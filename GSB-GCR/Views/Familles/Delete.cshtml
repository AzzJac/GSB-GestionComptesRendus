@model GSB_GCR.Models.Famille

@{
    ViewData["Title"] = "Delete";
}

<h1>Supprimer</h1>

<h3>Êtes-vous sûr de supprimer ceci ?</h3>
<div>
    <h4>Famille</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FamLibelle)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FamLibelle)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="FamCode" />
        <input type="submit" value="Supprimer" class="btn btn-danger" /> |
        </br>
        <a asp-action="Index" class="btn btn-primary">Revenir à la liste</a>
    </form>
</div>
